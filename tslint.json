{
	"extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
	"linterOptions": {
		"exclude": [
			"config/**/*.js",
			"node_modules/**/*.ts"
		]
	},
	"defaultSeverity": "error",
    "rules": {
		"ordered-imports": false,
		"no-console":false,
		"object-literal-sort-keys":false,
		"jsx-no-lambda":false,
		"jsx-no-bind":false,
		"only-arrow-functions": [
			false
		],
		"jsx-key":false
    }
}
